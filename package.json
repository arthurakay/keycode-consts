{
  "name": "keycode-consts",
  "version": "0.1.0",
  "description": "A collection of key code constants and utility functions for JavaScript.",
  "main": "index.js",
  "author": "Zac Davis (@thatzacdavis)",
  "license": "MIT",
  "files": [
    "pkg"
  ],
  "scripts": {
    "pika:build": "npx pika build",
    "publish:local": "cd pkg && yarn pack && copyfiles *.tgz .. && del-cli *.tgz",
    "publish:pika": "npx pika publish",
    "version": "npx @pika/pack",
    "test": "echo 'No tests are currently configured.'"
  },
  "devDependencies": {
    "@pika/pack": "0.5.0",
    "@pika/plugin-build-node": "0.9.2",
    "@pika/plugin-build-types": "0.9.2",
    "@pika/plugin-build-web": "0.9.2",
    "@pika/plugin-standard-pkg": "0.9.2",
    "copyfiles": "2.2.0",
    "del": "5.1.0",
    "del-cli": "3.0.0",
    "typescript": "3.8.3"
  },
  "@pika/pack": {
    "pipeline": [
      [
        "@pika/plugin-standard-pkg",
        {
          "exclude": [
            "/tests/**"
          ]
        }
      ],
      [
        "@pika/plugin-build-node"
      ],
      [
        "@pika/plugin-build-web"
      ],
      [
        "@pika/plugin-build-types"
      ]
    ]
  }
}
